generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model tb_link_pendataan {
  id   Int     @id(map: "tb_link_pendataan_pk") @default(autoincrement())
  link String? @db.VarChar
}

model tb_program {
  id       Int       @id(map: "tb_program_pk") @default(autoincrement())
  judul    String?   @db.VarChar
  content  String?
  uploaded DateTime? @db.Timestamp(6)
  modified DateTime? @db.Timestamp(6)
}

model tb_misi {
  id      Int     @id(map: "tb_misi_pk") @default(autoincrement())
  content String?
}

model tb_visi {
  id      Int     @id(map: "tb_visi_pk") @default(autoincrement())
  content String?
}

model tb_galleri {
  id                Int                 @id(map: "tb_galleri_pk") @default(autoincrement())
  judul             String?             @db.VarChar
  link_folder_drive String?             @db.VarChar
  galleri_detail    tb_galleri_detail[]
}

model tb_galleri_detail {
  id         Int         @id(map: "tb_galleri_detail_pk") @default(autoincrement())
  id_galleri Int?
  path       String?     @db.VarChar
  galleri    tb_galleri? @relation(fields: [id_galleri], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "tb_galleri_detail_tb_galleri_fk")

  @@index([id_galleri])
}

model tb_berita {
  id        Int       @id(map: "tb_berita_pk") @default(autoincrement())
  judul     String?   @db.VarChar
  content   String?
  uploaded  DateTime? @db.Timestamp(6)
  modified  DateTime? @db.Timestamp(6)
  thumbnail String?   @db.VarChar
}
