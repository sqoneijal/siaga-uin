generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model tb_link_pendataan {
  id   Int     @id(map: "tb_link_pendataan_pk") @default(autoincrement())
  link String? @db.VarChar
}

model tb_program {
  id       Int       @id(map: "tb_program_pk") @default(autoincrement())
  judul    String?   @db.VarChar
  content  String?
  uploaded DateTime? @db.Timestamp(6)
  modified DateTime? @db.Timestamp(6)
}

model tb_misi {
  id      Int     @id(map: "tb_misi_pk") @default(autoincrement())
  content String?
}

model tb_visi {
  id      Int     @id(map: "tb_visi_pk") @default(autoincrement())
  content String?
}
